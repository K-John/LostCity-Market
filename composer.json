{
    "name": "k-john/lostcity-market",
    "type": "project",
    "description": "Lost City Market",
    "keywords": [],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "based/momentum-lock": "^0.3.0",
        "based/momentum-modal": "^0.5.0",
        "based/momentum-trail": "^0.3.0",
        "consoletvs/profanity": "^3.5",
        "guzzlehttp/guzzle": "^7.9",
        "inertiajs/inertia-laravel": "2.0",
        "intervention/image": "^3.11",
        "laravel/framework": "^11.0",
        "laravel/pulse": "^1.4",
        "laravel/reverb": "^1.0",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.18",
        "laravel/tinker": "^2.10",
        "predis/predis": "^3.2",
        "pusher/pusher-php-server": "^7.2",
        "socialiteproviders/discord": "^4.2",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-backup": "^9.3",
        "spatie/laravel-data": "^4.0",
        "spatie/laravel-typescript-transformer": "^2.5",
        "tymon/jwt-auth": "^2.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.5",
        "fakerphp/faker": "^1.24",
        "larastan/larastan": "^2.9",
        "laravel/breeze": "^2.3",
        "laravel/pint": "^1.20",
        "laravel/sail": "^1.41",
        "laravel/telescope": "^5.4",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest-plugin-laravel": "^3.1",
        "phpunit/phpunit": "^11.5",
        "spatie/laravel-ignition": "^2.9",
        "spatie/laravel-stubs": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan spatie-stub:publish --force"
        ],
        "helpers": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite",
            "@php artisan ide-helper:eloquent"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "pint": [
            "./vendor/bin/pint"
        ],
        "pest": [
            "./vendor/bin/pest"
        ],
        "stan": [
            "./vendor/bin/phpstan analyse"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
